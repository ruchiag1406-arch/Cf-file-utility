{
  "family": "cf-files-utility-task-new-v1",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::973811007952:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "cf-files-utility-new",
      "image": "<IMAGE_URI>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        { "name": "SERVER_PORT", "value": "8080" }
      ]
      // âœ… Removed ECS container health check
    }
  ]
}
